{
    "version": 2,
    "builds": [
      {
        "src": "src/server.ts",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/routes/(.*)"
      },
      {
        "src": "/(.*)",
        "dest": "src/server.ts"
      }
    ],
    "env": {
      "DATABASE_URL": "file:./dev.db"
    }
  }